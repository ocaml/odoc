(library
 (name odoc_xref2)
 (public_name odoc.xref2)
; (preprocess (pps landmarks.ppx --auto))
 (libraries compiler-libs.common odoc_model odoc_loader landmarks)
 )
(alias
 (name   runtest)
 (deps   (:x paths.md)
         (package odoc))
 (action (ignore-stderr (progn
           (run ocaml-mdx test --direction=infer-timestamp %{x})
           (diff? %{x} %{x}.corrected))
)))
